{
    "name": "atsu-maru/qms3_form_brick",
    "authors": [
        {
            "name": "ATSUMARU inc",
            "email": "system@atsu-maru.co.jp"
        }
    ],
    "config": {
        "platform": {
          "php": "5.4.0"
        }
    },
    "scripts": {
        "_u": [ "composer update --no-dev --ignore-platform-reqs" ],
        "test": [ "php phpunit-9.5.1.phar --testdox test" ]
    },
    "autoload": {
        "psr-4": {
            "QMS3\\Brick\\": "lib/",
            "QMS3\\BrickAdmin\\": "admin/lib/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@bitbucket.org:team_qh/digima-php-client.git"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "league/plates": "~3.3.0",
        "cocur/slugify": "~1.4.1",
        "justinrainbow/json-schema": "^5.2",
        "phpmailer/phpmailer": "^6.4",
        "symfony/polyfill-mbstring": "~1.19",
        "psr/log": "^1.1",
        "monolog/monolog": "~1.26",
        "mobiledetect/mobiledetectlib": "^2.8",
        "comvex-jp/digima-php-client": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5"
    }
}
